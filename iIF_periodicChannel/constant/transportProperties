FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              2e-04;
    rho             1000;
}

air
{
    transportModel  Newtonian;
    nu              2e-05;
    rho             20;
}


sigma               0.05;


